# Hub Chantier - Development Environment
# Copier vers .env.development et adapter si necessaire
#
# Usage: docker compose -f docker-compose.yml -f docker-compose.dev.yml --env-file .env.development up

# Application
APP_NAME="Hub Chantier (Dev)"
DEBUG=true

# PostgreSQL Database
POSTGRES_USER=hubchantier
POSTGRES_PASSWORD=hubchantier_dev
POSTGRES_DB=hub_chantier

# Security (dev only - change in production!)
SECRET_KEY=dev-secret-key-not-for-production-use-32chars
ACCESS_TOKEN_EXPIRE_MINUTES=480

# CORS - Origines autorisees
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:8080,http://localhost
